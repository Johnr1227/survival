package com.cool.particles;

import com.cool.particle.Particle;
import com.cool.tile.Tile;

public class ParticleTile extends ParticleType {
	
	public ParticleTile(float x, float y, float z, float velX, float velY, float velZ, float size, Tile t) {
		super(x, y, z, velX, velY, velZ, size, t.getTexture());
	}
	public ParticleTile(Tile t) {
		super(-0.5f, 0.5f, 0f, 0.5f, -0.5f, 0.5f, 0.1f, 0.3f, t.getTexture());
	}
	
}
