package com.cool.lib;

import java.io.IOException;
import java.nio.file.Files;
import java.nio.file.Paths;

public class IO {
	public static void write(String path, String data) {
        try {
            Files.write(Paths.get(path), data.getBytes());
        } catch (IOException e) {
            e.printStackTrace();
        }
    }
}
