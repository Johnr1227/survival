package com.cool.particle;

import com.cool.lib.Texture;

public class Particle {
	public float x;
	public float y;
	public float z;
	
	public float velX;
	public float velY;
	public float velZ;
	
	public float size;
	
	public Texture t;
	
	public Particle (float x, float y, float z, float velX, float velY, float velZ, float size, Texture t) {
		this.x = x;
		this.y = y;
		this.z=z;
		this.velX = velX;
		this.velY=velY;
		this.velZ=velZ;
		this.size = size;
		this.t = t;
	}
}
